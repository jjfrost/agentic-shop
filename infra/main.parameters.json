{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "principalName": {
      "value": "${AZURE_PRINCIPAL_NAME=toSet}"
    },
    "administratorLoginUser": {
      "value": "${ADMINISTRATOR_LOGIN_USER=rtbackend}"
    },
    "administratorLoginPassword": {
      "value": "${ADMINISTRATOR_LOGIN_PASSWORD=Agent2shop}"
    },
    "openAILocation": {
      "value": "${AZURE_OPENAI_LOCATION}"
    },
    "deployAzureOpenAI": {
      "value": "${DEPLOY_AZURE_OPENAI=true}"
    },
    "deployContainerApps": {
      "value": "${DEPLOY_AZURE_CONTAINERAPPS=true}"
    },
    "openAIChatHost": {
      "value": "${OPENAI_CHAT_HOST=azure}"
    },
    "chatModelName": {
      "value": "${AZURE_OPENAI_CHAT_MODEL=gpt-4o}"
    },
    "chatDeploymentName": {
      "value": "${AZURE_OPENAI_CHAT_DEPLOYMENT=gpt-4o}"
    },
    "chatDeploymentVersion":{
      "value": "${AZURE_OPENAI_CHAT_DEPLOYMENT_VERSION=}"
    },
    "chatDeploymentSku": {
      "value": "${AZURE_OPENAI_CHAT_DEPLOYMENT_SKU=GlobalStandard}"
    },
    "chatDeploymentCapacity":{
      "value": "${AZURE_OPENAI_CHAT_DEPLOYMENT_CAPACITY=50}"
    },
    "embedModelName":{
      "value": "${AZURE_OPENAI_EMBED_MODEL=text-embedding-3-small}"
    },
    "embedDeploymentName": {
      "value": "${AZURE_OPENAI_EMBED_DEPLOYMENT=text-embedding-3-small}"
    },
    "embedDeploymentVersion":{
      "value": "${AZURE_OPENAI_EMBED_DEPLOYMENT_VERSION=}"
    },
    "embedDeploymentSku": {
      "value": "${AZURE_OPENAI_EMBED_DEPLOYMENT_SKU=GlobalStandard}"
    },
    "embedDeploymentCapacity":{
      "value": "${AZURE_OPENAI_EMBED_DEPLOYMENT_CAPACITY=70}"
    },
    "containerRegistryName": {
      "value": "${AZURE_CONTAINER_REGISTRY_ENDPOINT}"
    }
  }
}
